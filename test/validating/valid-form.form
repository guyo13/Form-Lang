type number {}

component myform {}

component mynestedform {}

component my_comp {
    props {
        prop1
        prop2
        prop3
    }
}

form LaLaForm {
    comp myform
    field f1 {
        state number default "33"
        comp my_comp
    }
    field f2 {
        state number[] default "[1,2,3]" as expression
        comp my_comp prop1="value1" prop2="value2" prop3="3" as expression
    }
    form nested {
        comp mynestedform
    }
}


form lala2 {
    comp myform
    form nested {
        comp mynestedform
    }
}
